{
  "name": "@storybook/preset-solid-js-webpack",
  "version": "7.0.0-alpha.10",
  "description": "Storybook for SolidJS: Develop SolidJS Component in isolation.",
  "keywords": [
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/main/presets/solid-js-webpack",
  "bugs": {
    "url": "https://github.com/storybookjs/storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/storybook.git",
    "directory": "presets/solid-js-webpack"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preset": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json",
      "types": "./package.json"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepare": "esrun ../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/core-webpack": "7.0.0-alpha.8",
    "@types/node": "^14.14.20 || ^16.0.0",
    "core-js": "^3.8.2",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.2",
    "solid-js": "^1.4.5"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "solid-js": "^1.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundlerEntrypoint": [
    "./src/index.ts"
  ]
}
